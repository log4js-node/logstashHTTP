{
  "name": "@log4js-node/logstash-http",
  "version": "1.1.0",
  "description": "Logstash HTTP Appender for log4js-node",
  "homepage": "https://log4js-node.github.io/log4js-node/",
  "files": [
    "lib",
    "types"
  ],
  "keywords": [
    "logging",
    "log",
    "log4j",
    "node",
    "logstash"
  ],
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "author": "Gareth Jones <gareth.nomiddlename@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/log4js-node/logstashHTTP.git"
  },
  "bugs": {
    "url": "http://github.com/log4js-node/logstashHTTP/issues"
  },
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg",
    "prettier:fix": "prettier --write .",
    "pretest": "prettier --check . && eslint \"lib/**/*.js\" \"test/**/*.js\"",
    "test": "tap test/tap/**/*.js --100",
    "typings": "tsc -p types/tsconfig.json",
    "codecov": "tap 'test/tap/**/*.js' --cov --coverage-report=lcov && codecov"
  },
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@log4js-node/sandboxed-module": "^2.2.1",
    "@types/node": "^18.11.12",
    "codecov": "^3.0.0",
    "conventional-changelog": "^3.1.25",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "log4js": "^6.5.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.0",
    "tap": "^16.3.2",
    "typescript": "^4.7.2",
    "validate-commit-msg": "^2.14.0"
  },
  "browser": {
    "os": false
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "example",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 72,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "\n# allowed type: feat, fix, docs, style, refactor, example, perf, test, chore, revert\n# subject no more than 50 chars\n# a body line no more than 72 chars"
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js"
    ],
    "require": [
      "./test/sandbox-coverage"
    ]
  }
}
